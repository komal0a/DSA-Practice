class Solution{	
	public:
		 int xorUpto(int x) {
        if (x % 4 == 0) return x;
        if (x % 4 == 1) return 1;
        if (x % 4 == 2) return x + 1;
        return 0; // x % 4 == 3
    }

    int findXOR(int L, int R) {
        return xorUpto(R) ^ xorUpto(L - 1);;
        else return {b, a};
		}
};